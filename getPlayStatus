os.loadAPI("mc/json")

str = http.get("https://limitless-hollows-7191.herokuapp.com/runners/1").readAll()

		obj = json.decode(str)
		play = obj.active
    
		if play == "true" then
			print("Power ON!")
			rs.setAnalogOutput("right",15)
		else
			print("Power OFF.....")
			rs.setAnalogOutput("right",0)
		end
