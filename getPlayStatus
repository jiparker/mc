os.loadAPI("mc/json")

str = http.get("https://gist.github.com/jiparker/b90c5fc666d7d7e9b8e3/raw").readAll()

		obj = json.decode(str)
		play = obj.play
    
		if play == "true" then
			print("Power ON!")
			rs.setAnalogOutput("right",15)
		else
			print("Power OFF.....")
			rs.setAnalogOutput("right",0)
		end
